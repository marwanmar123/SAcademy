@model List<SAcademy.Models.Menu>


@foreach (var menu in Model)
{
    var size = string.Concat(menu.SizeFooter, "px");
    <div class="d-flex flex-column">
        <a class="" href="@menu.Link" style="color:@menu.ColorFooter; font-size:@size; font-family:@menu.FontFamily;">@menu.TitleMenu</a>
    </div>
}
