@model List<SAcademy.Models.Contact>


<div class="d-flex justify-content-between align-items-center p-1 text-light navcontact">
    <div class="d-flex">
        @foreach (var c in Model)
        {
            <div class="mr-4 itemNvcontact">
                <img style="width:26px;" src="~/img/phoneNav.png">
                <a class="text-decoration-none text-light" href="tel:@c.Call">@c.Call</a>
            </div>
            <div class="mr-4 itemNvcontact">
                <img style="width:20px;" src="~/img/LocNav.png">
                <a class="text-decoration-none text-light" href="https://goo.gl/maps/rTuLwJPL42xfjJ3HA"
                target="_blank">@c.Localisation</a>
            </div>
            <div class="mr-4 itemNvcontact">
                <img style="width:20px;" src="~/img/mailNav.png">
                <a class="text-decoration-none text-light" href="mailto:@c.Email">@c.Email</a>
            </div>
        }
    </div>
    <div class="">
        <a target="_blank" href="https://web.facebook.com/profile.php?id=100063864017100"><img style="width:50px;"
                src="~/img/fbnav.png" /></a>
        <a target="_blank" href="https://twitter.com/SimploncoMaroc"><img style="width:50px;"
                src="~/img/twitnav.png" /></a>
        <a target="_blank" href="https://www.linkedin.com/company/simplon-maghreb/?viewAsMember=true"><img
                style="width:50px;" src="~/img/linkednav.png" /></a>
        <a target="_blank" href="https://www.youtube.com/%40simplonmaghreb8023/featured"><img style="width:50px;"
                src="~/img/ytbnav.png" /></a>
        <a target="_blank" href="https://www.instagram.com/simplonmaghreb/"><img style="width:50px;"
                src="~/img/instanav.png" /></a>
    </div>
</div>
