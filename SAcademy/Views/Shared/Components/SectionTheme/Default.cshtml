@model List<SAcademy.Models.SectionTheme>


<div class="w-100" id="">

    @foreach (var item in Model)
    {
        if (item.Visible == true)
        {
            var size = string.Concat(item.TitleSize, "px");
            <p class="text-center p-3 Titles m-0"
        style="color:@item.TitleColor; font-size:@size; font-weight:bold; font-family:@item.FontFamily;">@item.Title</p>
            <div class="pb-2">@Html.Raw(item.Content)</div>
        }
    }

</div>


